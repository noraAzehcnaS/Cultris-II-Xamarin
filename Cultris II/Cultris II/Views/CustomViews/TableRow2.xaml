<?xml version="1.0" encoding="UTF-8" ?>
<ContentView
    x:Class="Cultris_II.Views.CustomViews.TableRow2"
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Name="this">
    <ContentView.Content>
        <Grid
            Margin="0,14,0,0"
            BindingContext="{x:Reference this}"
            ColumnDefinitions="1*,3*"
            WidthRequest="100">
            <Label
                Grid.Column="0"
                FontSize="Body"
                Text="{RelativeSource Text1}"
                VerticalOptions="Center" />
            <Label
                Grid.Column="1"
                FontSize="Body"
                Text="{Binding Text2}"
                VerticalOptions="Center" />
        </Grid>
    </ContentView.Content>
</ContentView>